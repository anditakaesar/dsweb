    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "^1.4.5",
    "date-fns": "^2.22.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "moment": "^2.29.1",
    "pdf-lib": "^1.16.0",
    "puppeteer": "^10.1.0",
